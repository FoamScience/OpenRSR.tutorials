FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volScalarFieldValue K 1e-10
);



regions
(
    // Impermeable region
    zoneToCell
    {
      name c0;
      fieldValues
     (
        volScalarFieldValue K 1e-9
     );
    }   
);



