FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     impesFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

deltaT          100;  // 3 days
endTime         302400000; // 3500 days, 9.6 years
//endTime         $deltaT; // 3500 days, 9.6 years

//deltaT          100;  // 3 days

writeControl    adjustableRunTime;

writeInterval   8640000; // each year
//writeInterval    100; // each year

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

dSmax           0.03;
Cm              0.75;
//dSmax           0.01;// needed with capillary effect

maxDeltaT  864000; // 1days

//DebugSwitches
//{
//    fvSchemes 1;
//}

