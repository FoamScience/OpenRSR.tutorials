FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "constant";
  object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water oil);

water{}
oil{}

relativePermeabilityModel   BrooksCorey;
BrooksCoreyCoeffs
{
    water.m        2;
    oil.m          2;
    water.alphaIrr 0.2;
    oil.alphaRes   0.2;
    water.krMax    0.9;
    oil.krMax      0.5;
    canonicalPhase water;
}

capillaryPressureModel      none;

